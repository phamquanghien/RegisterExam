<div class="row">
    <div class="col-md-4">
        <form asp-action="Upload" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div>
                <label>Security Key:</label>
                <input class="w3-input w3-border w3-round" type="password" name="checkKey" />
            </div>
            <div class="w3-margin-top">
                <label>Upload file excel:</label>
                <input name="file" type="file" class="form-control">
            </div>
            <select class="w3-select w3-border w3-round w3-white w3-padding w3-margin-top" name="Subject">
                <option value="THVPNC">Tin học Văn phòng Nâng cao</option>
                <option value="TMDT">Thương mại Điện tử</option>
                <option value="JavaOOP">Lập trình Hướng Đối tượng với Java</option>
            </select>
            <div class="form-group w3-margin-top">
                <input type="submit" value="Upload" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-info">Danh sách sinh viên</a>
            </div>
        </form>
    </div>
</div>
@ViewBag.mess
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}